{
   "name": "postgres-server",
   "description": "Postgres database server",
   "default_attributes": {
    "postgresql" : {
      "version" : "13"
    },
    "monit_configs-tlq" : {
      "postgres" : {
        "pidfile" : "/var/run/postgresql/13-main.pid"
      }
    }
   },
   "json_class": "Chef::Role",
   "run_list": [
      "role[server]",
      "postgresql_wrapper::default",
      "monit_configs-tlq::postgres"
   ],
   "chef_type": "role"
}
